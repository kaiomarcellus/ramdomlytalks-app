        <attr name="textViewFontMedium" format="boolean" />
        <attr name="textViewFontBold" format="boolean" />
        <attr name="textViewFontExtraBold" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RadioButtonComponent">
        <attr name="radioButtonFontMedium" format="boolean" />
        <attr name="radioButtonFontBold" format="boolean" />
        <attr name="radioButtonFontExtraBold" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InputComponent">
        <attr name="android:hint" />
        <attr name="android:text" />
        <attr name="android:inputType" />
